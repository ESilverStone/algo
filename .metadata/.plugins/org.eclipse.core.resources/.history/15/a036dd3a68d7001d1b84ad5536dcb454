import java.util.Scanner;

public class a형모의_수영장 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		
		for(int t=0; t<T; t++) {
			
			int[] cost = new int[4];
			
			for(int i=0; i<4; i++) {
				cost[i] = sc.nextInt();
			}
			
			int[] month = new int[12];
			
			for(int i=0; i<12; i++) {
				month[i] = sc.nextInt();		
			}
			
			
		}
		
	}
}
