package 보드게임컵;

import java.util.Scanner;

public class p27162_YachtDice {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		// 족보 입력 받기
		String[] str = sc.next().split("");
		
		// 주사위 세개
		int[] dice = new int[7];
		for(int i=0; i<3; i++) {
			dice[sc.nextInt()]++;
		}
		
		
		// 나머지 주사위 두개
		for(int i=1; i<=6; i++) {
			for(int j=1; j<=6; j++) {
				dice[i]++;
				dice[j]++;
				
		}
		
	}
}
