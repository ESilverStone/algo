import java.util.Arrays;
import java.util.Scanner;

public class p2747_피보나치수 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int N = sc.nextInt();
		
		dp = new int[N+1];
		
		Arrays.fill(dp, -1);
		
		dp[0] = 0;
		dp[1] = 1;
		
		
	}
	static int[] dp;
	public static int fibo(int N) {
		
		if(dp[N] == -1) {
			
			return dp[N] = dp[N-1] + dp[N-2];
		}
		
		
		return dp[N];
		
	}
}
